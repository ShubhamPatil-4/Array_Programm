package Array;
import java.util.*;
public class vowel {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/* Create in array which is going takes use input and check which elements in array is vowel. If it is vowel count how many vowel are present*/
		Scanner sc=new Scanner(System.in);
		
		char char_arr[]=new char[5];
		System.out.println("Enetr any char value:-");
		
		for(int i=0;i<char_arr.length;i++)
		{
			char_arr[i]=sc.next().charAt(0);
			
		}
		int count=0;
		for(int i=0;i<char_arr.length;i++)
		{
			if(char_arr[i]=='a'||char_arr[i]=='e'||char_arr[i]=='i'||char_arr[i]=='o'||char_arr[i]=='u')
			{
				count++;
			}
		}
		
		System.out.println("The number of vowels in given array is "+count);
	}

}
